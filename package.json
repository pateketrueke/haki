{
  "name": "haki",
  "version": "0.1.1",
  "main": "lib/index.js",
  "bin": "bin/haki",
  "description": "Super fast generator!",
  "author": "Alvaro Cabrera <pateketrueke@gmail.com>",
  "license": "MIT",
  "config": {
    "mocha": "--compilers coffee:coffeescript/register --recursive ./spec --timeout 45000"
  },
  "scripts": {
    "dev": "yarn spec -- -w",
    "spec": "_mocha $npm_package_config_mocha",
    "cover": "istanbul cover --root lib _mocha -- $npm_package_config_mocha",
    "lint": "eslint lib && coffeelint spec",
    "cover:up": "codecov --file=coverage/lcov.info --disable=gcov -e TRAVIS_NODE_VERSION",
    "test": "yarn lint && yarn spec"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "change-case": "^3.0.1",
    "clean-stack": "^1.1.1",
    "download-github-repo": "^0.1.3",
    "enquirer-prompt-input": "^0.4.0",
    "fs-extra": "^2.0.0",
    "mustache": "^2.3.0",
    "pluralize": "^3.1.0",
    "prompt-checkbox": "^0.4.2",
    "prompt-confirm": "^0.2.1",
    "prompt-expand": "^0.1.0",
    "prompt-list": "^0.1.1",
    "prompt-password": "^0.1.1",
    "prompt-question": "^0.4.0",
    "prompt-radio": "^0.3.1",
    "prompt-rawlist": "^0.2.1",
    "readline-ui": "^0.1.2",
    "rimraf": "^2.6.1",
    "wargs": "^0.2.1"
  },
  "devDependencies": {
    "codecov": "^1.0.1",
    "coffeelint": "^1.16.0",
    "coffeescript": "^1.12.4",
    "eslint": "^3.14.0",
    "expect": "^1.20.2",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "ttys": "^0.0.3"
  }
}
